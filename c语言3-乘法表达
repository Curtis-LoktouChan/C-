#define _CRT_SECURE_NO_WARNINGS
/*VS2019专用*/
# include<stdio.h>
int product (int,int);/*函数定义写main后面，要先声明。声明就是函数定义加分号
声明不用写形参类型&形参符号（int a），（因为a只有在函数声明这一语句中有效）只用写形参类型（int），记得加分号。*/
int main(void)
{int x,y,s;
scanf("%d%d",&x,&y);
s=product(x,y);
printf("The mul is:%d",s);
return 0;
}
int product(int a, int b)/*定义product，
函数返回值类型（int）&函数名&形式参数表（参数类型&变量名...)*/
{
	int mul;
	mul = a * b;
	return mul;
}


//C语言任务一：20乘以10 
#include<stdio.h>
int main()
{ 
     int x,y,p;
	 x=10,y=20;
     p=product(x,y) ;

     printf("The product is :%d",p) ;
 }
    
	  int product(int a ,int b ) 
     {
	 int c;
     c=a*b ;
     return c; 
} 

# include<stdio.h>
int product (int,int);//函数声明记得加分号
int main(void)
{int x,y,s;
scanf("%d%d",&x,&y);
s=product(x,y);
printf("The mul is:%d",s);
return 0;
}
int product(int a, int b)
{
	int mul;
	mul = a * b;
	return mul;
}
