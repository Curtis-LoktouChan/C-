#define _CRT_SECURE_NO_WARNINGS
/*VS2019专用*/
# include<stdio.h>
int product (int,int);
int main(void)
{int x,y,s;
scanf("%d%d",&x,&y);
s=product(x,y);
printf("The mul is:%d",s);
return 0;
}
int product(int a, int b)
{
	int mul;
	mul = a * b;
	return mul;
}


//C语言任务一：20乘以10 
#include<stdio.h>
int main()
{ 
     int x,y,p;
	 x=10,y=20;
     p=product(x,y) ;

     printf("The product is :%d",p) ;
 }
    
	  int product(int a ,int b ) 
     {
	 int c;
     c=a*b ;
     return c; 
} 
